<table class="fee_table">
  <% sheet1.each_with_index do |row, i| %>
    <% if i == 0 %>
      <caption><%= row[0] %></caption>
    <% else %>
      <%= row_tag(i, row[0]) do %>
        <% row.each_with_index do |col, j| %>
          <% next if j == 0 %>
          <%= table_cell( col, i, j, currency || "Q" ) %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
</table>
