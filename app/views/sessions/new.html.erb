<h1>Autenticación</h1>

<%= form_tag sessions_path do %>
  <ul>
    <li>
      <%= label_tag :username, "usuario" %>
      <%= text_field_tag :username, params[:username] %>
    </li><li>
      <%= label_tag :password, "contraseña" %>
      <%= password_field_tag :password %>
    </li>
  </ul>
  <%= submit_tag "acceder" %>
<% end %>
